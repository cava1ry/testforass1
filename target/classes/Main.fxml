<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="638.0" prefWidth="834.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
    <children>
        <MenuBar prefHeight="32.0" prefWidth="834.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onMenuNew" text="New" />
                        <MenuItem mnemonicParsing="false" onAction="#onMenuOpen" text="Open" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Manage">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onMenuSave" text="Save" />
                        <MenuItem mnemonicParsing="false" onAction="#onMenuPrint" text="Print" />
                        <MenuItem mnemonicParsing="false" onAction="#onMenuExit" text="Exit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onMenuAbout" text="About" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Search">
                    <items>
                        <MenuItem mnemonicParsing="false">
                            <graphic>
                                <ButtonBar prefHeight="40.0" prefWidth="400.0">
                                    <buttons>
                                        <TextField fx:id="searchContent" />
                                        <Button mnemonicParsing="false" onAction="#onButtonSearch" text="Search" />
                                        <Button mnemonicParsing="false" onAction="#onButtonNext" text="Next" />
                                    </buttons>
                                </ButtonBar>
                            </graphic>
                        </MenuItem>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="View" />
            </menus>
        </MenuBar>
        <TextArea fx:id="fileContent" layoutY="32.0" prefHeight="615.0" prefWidth="834.0" />
    </children>
</AnchorPane>

